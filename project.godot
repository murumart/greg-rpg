; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Greg RPG"
config/version="0.8.1"
run/main_scene="res://scenes/gui/scn_main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Murumart"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/use_filter=false
config/icon="res://sprites/spr_icon.png"

[autoload]

DIR="*res://autoload/scn_directory.tscn"
DAT="*res://autoload/scn_data.tscn"
SOL="*res://autoload/scn_screen_overlay.tscn"
OPT="*res://autoload/scn_options.tscn"
LTS="*res://autoload/scn_level_transition.tscn"
SND="*res://autoload/scr_sound.gd"
ELM="*res://autoload/scr_elements.gd"

[debug]

gdscript/warnings/standalone_expression=2
gdscript/warnings/standalone_ternary=2
gdscript/warnings/integer_division=0
shapes/collision/shape_color=Color(1, 1, 1, 0.419608)

[display]

window/size/viewport_width=160
window/size/viewport_height=120
window/size/extend_to_title=true
window/size/window_width_override=959
window/size/window_height_override=720

[editor]

movie_writer/mix_rate=41000
movie_writer/movie_file="D:/godoot/GREGRPG/.exports/movie/recording.avi"

[file_customization]

folder_colors={
"res://resources/": "teal",
"res://resources/characters/": "yellow",
"res://resources/items/": "green",
"res://resources/rewards/": "blue",
"res://resources/spirits/": "purple",
"res://scenes/": "blue",
"res://scenes/rooms/": "orange"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/custom="res://resources/thm_main_ui.tres"

